# Porta do servidor
PORT=3000

# URLs base
API_BASE=https://registro.br/tecnologia/ferramentas/consulta-whois/
WS_BASE=ws://localhost:3000

# Timeouts (em milissegundos)
TIMEOUT_REGISTRO_BR=3000
TIMEOUT_WHOIS=5000
TIMEOUT_DNS=2000

# Configurações de cache
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Configurações de concorrência
MAX_PARALLEL_CHECKS=10
BATCH_SIZE=50

# Configurações de retry
MAX_RETRIES=3
RETRY_DELAY=1000
RETRY_BACKOFF_FACTOR=2

DOLPHIN_API_KEY=your_dolphin_api_key_here 